<tr>
  <td><%= review.title %></td>
  <td>
    <%= form_tag admin_confirm_review_path(review.id), method: :patch do %>
      <%= check_box_tag :is_confirm, {}, category.is_confirm,
        onclick: "changeStatus(this)" %>
      <%= t "admin.manager.review.is_confirm" %>
    <% end %>
  </td>
  <td>
    <%= link_to admin_category_review_review_path(@category_review, review),
      method: :delete,
      data: {confirm: t("admin.manager.review.destroy_confirm")} do %>
      <span class="glyphicon glyphicon-trash"></span>
    <% end %>
  </td>
</tr>
