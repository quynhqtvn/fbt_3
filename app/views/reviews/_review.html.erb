<% if review.is_confirm == true %>
<div class="col-md-4 project">
  <div class="projectInfo">
    <div class="clearfix"> </div>
    <h4><%= link_to review.title, review %></h4>
    <h4><%= review.category_review.title %></h4>
    <p><%= review.user.name %> | <%= l review.created_at, format: :short %></p>
  </div>
</div>
<% end %>
